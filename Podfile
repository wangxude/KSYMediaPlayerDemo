# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'KSYMediaPlayerDemo' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
   pod 'libksygpulive/KSYGPUResource'
   pod 'libksygpulive/libksygpulive’,'~> 2.9.3'
   pod 'Masonry'
   pod 'JSONModel'
   pod 'SDWebImage'
   pod 'Toast'
   pod 'Bugly'
   pod  ‘AFNetworking’
   # YYImage 仅demo中用于导入gif和apng的格式的图片用到了
   # 不需要支持这些格式的话,不需要添加YYImage的依赖
   pod 'YYImage'
   pod 'YYCache'
   pod 'ZipArchive'
  # Pods for KSYMediaPlayerDemo

  target 'KSYMediaPlayerDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'KSYMediaPlayerDemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
